PROCEDURE extract1

DECLARE &CNT1  INTEGER
DECLARE &STR1  STRING(8)

    EXTRACT &CNT1 = RECORDS USED BY XYZ  IN LIBIN ON VOLIN     
    EXTRACT &STR1 = RECORDS USED BY XYZ  IN LIBIN ON VOLIN     
    prompt "XYZ: CNT1="!!&CNT1 !! " STR1="!!&STR1

    EXTRACT &CNT1 = RECORDS USED BY EMPTY  IN LIBIN ON VOLIN     
    EXTRACT &STR1 = RECORDS USED BY EMPTY  IN LIBIN ON VOLIN     
    prompt "EMPTY: CNT1="!!&CNT1 !! " STR1="!!&STR1
