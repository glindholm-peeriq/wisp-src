      *"HL" - function
           MOVE  "HL" TO FUNCTION-TYPE.
           MOVE  SPACES TO FORMATTED-DATE-STRING. 
           CALL  "DATE" USING FUNCTION-TYPE,
                              FORMATTED-DATE-STRING.
           DISPLAY "  Format          AAAAAAAAA   BBBBBBBBBBBBBBBBBBB     
      -      "   CCCCCCCC                  (DATE) TODAY IS:",
             FORMATTED-DATE-STRING.

           MOVE  "HL" TO FUNCTION-TYPE.
           MOVE  SPACES TO FORMATTED-DATE-STRING. 
           CALL  "DATE2" USING FUNCTION-TYPE,
                              FORMATTED-DATE-STRING.  
           DISPLAY "  Format         AAAAAAAAAAA BBBBBBBBBBBBBBBBBBBBB     
      -      "  CCCCCCCC                 (DATE2) TODAY IS:",
             FORMATTED-DATE-STRING.
      *END "HL"

