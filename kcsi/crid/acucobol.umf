
#------------------------------------------------------------------
# @(#)acucobol.umf	1.4 6/13/94
#Make for a single acucobol module
#------------------------------------------------------------------

CCBLDBG= -Zd -Za

CCBLFLAGS=-da4
COBOL=ccbl

.SUFFIXES:	.cbo .dbg .wcb

.wcb.cbo:
	wisp -VACU $*.wcb
	$(COBOL) $(CCBLFLAGS) -o $*.cbo $*.cob


.wcb.dbg:
	wisp -VACU $*.wcb
	$(COBOL) $(CCBLFLAGS) $(CCBLDBG) -o $*.dbg $*.cob


