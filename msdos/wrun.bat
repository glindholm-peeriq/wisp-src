@ECHO OFF
REM             This .BAT file will start a wisp COBOL module.

SET PGM=%1
IF %PGM%!==! GOTO NOPROG
SHIFT
SET COMLINE=

:TOPCL
IF %1!==! GOTO ENDCL
SET COMLINE=%COMLINE% %1
SHIFT
GOTO TOPCL

:ENDCL
CALL WISPRUN.BAT N WISPRTS.EXE %PGM% %COMLINE%
GOTO END

:NOPROG
ECHO Program name required.

:END
